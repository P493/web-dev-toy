{
    "compileOnSave": false,
    "compilerOptions": {
        "target": "ES6",
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "baseUrl": "./",
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "declaration": true
    },
    "include": [
        "redux"
    ]
}